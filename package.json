{
  "name": "@lukiffer/recipe-catalog",
  "private": true,
  "version": "1.0.0-alpha",
  "author": "Luke Fritz <lukefritz@gmail.com",
  "description": "Web app for cataloging recipes and meal planning.",
  "license": "MIT",
  "scripts": {
    "build:site:appspec": "cp ./tools/aws/appspec.yml ./dist/",
    "build:site:dev": "npm run clean && webpack --config tools/webpack/webpack.dev.js --progress --profile",
    "build:site:prod": "npm run clean && webpack --config tools/webpack/webpack.prod.js  --progress --profile --bail && npm run build:site:appspec",
    "clean": "rimraf .tmp dist compiled dll",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint 'src/**/*.ts'",
    "lint:sass": "sass-lint -v",
    "ngcompile": "node_modules/.bin/ngc -p tools/ngc/tsconfig.aot.json",
    "server:dev": "webpack-dev-server --config tools/webpack/webpack.dev.js --open --progress --profile --watch --content-base src/ --inline --hot",
    "server:prod": "npm run build:site:prod && http-server dist -c-1 --cors",
    "watch": "npm run server:dev",
    "prebuild": "rimraf .package .tmp",
    "postinstall": "webdriver-manager update"
  },
